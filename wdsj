local library = loadstring(game:HttpGet(('https://raw.githubusercontent.com/wucan114514/an/refs/heads/main/lua')))()

local Window = library:CreateWindow("安脚本", "极速传奇", 10044538000)

local Tab = Window:CreateTab("功能列表")

local Page = Tab:CreateFrame("个人数据")

local ll = Page:CreateLabel("步数")
spawn(function()
while wait() do
pcall(function()
ll:UpdateLabel("fps"..math.floor(1 / game:GetService("RunService").RenderStepped:Wait()))
end)
end
end)

TextBox = Page:CreateBox("吸球倍数", 10044538000, function(arg)
xqcs = arg
end)

Label = Page:CreateLabel("城市区域（需要去城市，不可直接传送)")

_G.zdxlq1 = true

function zdxlq1()
	while _G.zdxlq1 == true do
	wait()
   	for i=1, (""..xqcs) do
local A_1 = "collectOrb"
local A_2 = "Yellow Orb"
local A_3 = "City"
local Event = game:GetService("ReplicatedStorage").rEvents.orbEvent
Event:FireServer(A_1, A_2, A_3)	
end			
	end
end

Toggle = Page:CreateToggle("黄球", "城市黄球", function(arg)
    _G.zdxlq1 = arg
		zdxlq1()
end)

