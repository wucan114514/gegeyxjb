local Tab = Window:CjLb({
	Name = "💪快速刷重生💪",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

_G.qzcs = true

function qzcs()
	while _G.qzcs == true do
	wait()
    	game.Players.LocalPlayer.Character.HumanoidRootPart.CFrame = CFrame.new(-8625.9296875, 13.566278457641602, -5730.4736328125)
	end
end

Tab:TianJiaToggle({
	Name = "自动传送到肌肉之王",
	Default = false,
	Callback = function(Value)
		_G.qzcs = Value
		qzcs()
	end    
})

yldll = false

Tab:TianJiaToggle({
	Name = "自动锻炼",
	Default = false,
	Callback = function(Value)
	yldll = Value
		game:GetService("RunService").Stepped:connect(
		function()
			pcall(
				function()
					if yldll then
					local args = {
    [1] = "rep"
}

game:GetService("Players").LocalPlayer.muscleEvent:FireServer(unpack(args))
						if yldll then
							game.Players.LocalPlayer.Character.Humanoid:EquipTool(
								game:GetService("Players").LocalPlayer.Backpack.Weight
							)
						end
					end
				end
			)
		end
		)
	end    
})

_G.sd = true

function sd()
	while _G.sd == true do
	wait(3)
   local args = {
    [1] = "changeSize",
    [2] = 2
}

game:GetService("ReplicatedStorage").rEvents.changeSpeedSizeRemote:InvokeServer(unpack(args))

	end
end

Tab:TianJiaToggle({
	Name = "自动设置大小为2",
	Default = false,
	Callback = function(Value)
		_G.sd = Value
		sd()
	end    
})

