_G.autocs = true

function autocs()
	while _G.autocs == true do
	wait()
	local A_1 = "rebirthRequest"
local Event = game:GetService("ReplicatedStorage").rEvents.rebirthEvent
Event:FireServer(A_1)		
	end
end

Tab:TianJiaToggle({
	Name = "自动重生",
	Default = false,
	Callback = function(Value)
	_G.autocs = Value
		autocs()
	end  
})

_G.autobushu = true

function autobushu()
	while _G.autobushu == true do
wait()	
for i=1, 7500 do
     local A_1 = "collectOrb"
local A_2 = "Red Orb"
local A_3 = "City"
local Event = game:GetService("ReplicatedStorage").rEvents.orbEvent
Event:FireServer(A_1, A_2, A_3)	
end		
	end
end

Tab:TianJiaToggle({
	Name = "自动获取步数(易卡)",
	Default = false,
	Callback = function(Value)
	_G.autobushu = Value
		autobushu()
	end  
})

_G.autobaoshi = true

function autobaoshi()
	while _G.autobaoshi == true do
wait()	
for i=1, 1000 do
	local A_1 = "collectOrb"
local A_2 = "Gem"
local A_3 = "City"
local Event = game:GetService("ReplicatedStorage").rEvents.orbEvent
Event:FireServer(A_1, A_2, A_3)	
end			
	end
end

Tab:TianJiaToggle({
	Name = "自动获取宝石(易卡)",
	Default = false,
	Callback = function(Value)
	_G.autobaoshi = Value
		autobaoshi()
	end  
})

