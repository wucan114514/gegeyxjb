_G.automs = true

function automs()
	while _G.automs == true do
	wait()
			local args = {
    [1] = false,
    [2] = "Basic Crate"
}

game:GetService("ReplicatedStorage").common.packages._Index:FindFirstChild("sleitnick_knit@1.5.1").knit.Services.CrateService.RF.Process:InvokeServer(unpack(args))
	end
end

