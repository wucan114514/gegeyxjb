local Tab = Window:CjLb({
	Name = "⚡自动功能⚡️",
	Icon = "rbxassetid://4483345998",
	PremiumOnly = false
})

sjzdzb = false

Tab:TianJiaToggle({
	Name = "自动哑铃锻炼",
	Default = false,
	Callback = function(Value)
		sjzdzb = Value
		game:GetService("RunService").Stepped:connect(
		function()
			pcall(
				function()
					if sjzdzb then
					game:GetService("ReplicatedStorage").Events.Weight:FireServer()
						if sjzdzb then
							game.Players.LocalPlayer.Character.Humanoid:EquipTool(
								game:GetService("Players").LocalPlayer.Backpack.Weight
							)
						end
					end
				end
			)
		end
		)
	end    
})

_G.sjhjyl = true

function sjhjyl()
	while _G.sjhjyl == true do
	wait()
game:GetService("ReplicatedStorage").Events.Golden:FireServer()

	end
end

Tab:TianJiaToggle({
	Name = "自动黄金哑铃锻炼（需拥有黄金哑铃）",
	Default = false,
	Callback = function(Value)
	_G.sjhjyl = Value
		sjhjyl()		
	end    
})

_G.sjyt = true

function sjyt()
	while _G.sjyt == true do
	wait()
local args = {
    [1] = game:GetService("Players").LocalPlayer.PlayerGui.PullUps.Lifting,
    [2] = game:GetService("Players").LocalPlayer.PlayerGui.PullUps:FindFirstChild("Pull Up")
}

game:GetService("ReplicatedStorage").Events.MachineGain:FireServer(unpack(args))

	end
end

Tab:TianJiaToggle({
	Name = "自动引体向上（需使用引体向上）",
	Default = false,
	Callback = function(Value)
	_G.sjyt = Value
		sjyt()		
	end    
})

_G.sjsd = true

function sjsd()
	while _G.sjsd == true do
	wait()
local args = {
    [1] = workspace.PUNCH.PUNCHRES.TWEEN.RESNEED,
    [2] = workspace.PUNCH.PUNCHRES.TWEEN.RESGAIN,
    [3] = workspace.PUNCH.PUNCHRES.TWEEN
}

game:GetService("ReplicatedStorage").Events.Punch:InvokeServer(unpack(args))

	end
end

Tab:TianJiaToggle({
	Name = "自动打沙袋",
	Default = false,
	Callback = function(Value)
	_G.sjsd = Value
		sjsd()		
	end    
})

