C.Size = Vector3.new(1000000, 100, 10000000)
C.Anchored = true
	end  
})

srzc = false

Tab:TianJiaToggle({
	Name = "自动吃食物",
	Default = false,
	Callback = function(Value)
	srzc = Value
		game:GetService("RunService").Stepped:connect(
		function()
			pcall(
				function()
					if srzc then
				local args = {
    [1] = {
        [1] = "GainMass",
        [2] = 0
    }
}

game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))

						if srzc then
						for i,v in pairs(game.Players.LocalPlayer.Backpack:GetChildren()) do 
                    if v.ClassName == "Tool" and v:FindFirstChild("Income_Tool_Script") then 
                        game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)
					end
                        if v.ClassName == "Tool" and v:FindFirstChild("Income_Tool_Script") then 
                            game.Players.LocalPlayer.Character.Humanoid:EquipTool(v)                            
                        end
                end
						end
					end
				end
			)
		end
		)
	end  
})

_G.srzcs = true

function srzcs()
	while _G.srzcs == true do
	local args = {
    [1] = {
        [1] = "SellMass"
    }
}

game:GetService("ReplicatedStorage").RemoteEvent:FireServer(unpack(args))
		
	end
end

Tab:TianJiaToggle({
	Name = "自动出售",
	Default = false,
	Callback = function(Value)
		_G.srzcs = Value
		    srzcs()
	end    
})