local Tab = Window:CjLb({
        Name = "公告•销毁UI",
        Icon = "rbxassetid://7733960981",
        PremiumOnly = false
})

Tab:TianJiaButton({
	Name = "传送到安全区域",
	Callback = function()
	local plr = game.Players
local lplr = plr.LocalPlayer
